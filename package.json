{
	"name": "barrett-penrod-site",
	"private": true,
	"scripts": {
		"dev": "doppler run -- next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint --dir .",
		"ci:build": "doppler run -p barrett-penrod-site -c stg -- next build",
		"ci:start": "doppler run -p barrett-penrod-site -c stg -- next start",
		"prisma:studio": "doppler run --mount .env -- npx prisma studio",
		"prisma:migrate": "doppler run --mount .env -- npx prisma migrate dev",
		"prisma:deploy": "doppler run --mount .env -- npx prisma migrate deploy",
		"prisma:generate": "doppler run --mount .env -- npx prisma generate",
		"prisma:format": "doppler run --mount .env -- npx prisma format",
		"vercel:dev": "vercel dev",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "doppler run -- vitest",
		"test:ui": "doppler run -- vitest --ui",
		"test:ci": "doppler run -- vitest run",
		"coverage": "doppler run -- vitest --coverage"
	},

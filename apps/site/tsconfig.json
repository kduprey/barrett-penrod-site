{
	"extends": "tsconfig/nextjs.json",
	"compilerOptions": {
		"plugins": [{ "name": "next" }],
		"baseUrl": ".",
		"paths": { "@/*": ["./*", "./src/*"] },
		"allowJs": true,
		"types": ["node", "vitest"]
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		".eslintrc.js"
	],
	"exclude": ["node_modules"]
}

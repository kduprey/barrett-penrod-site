{
	"name": "@bpvs/db",
	"version": "0.0.1",
	"private": true,
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"scripts": {
		"lint": "npx prisma format",
		"generate": "dotenv -e ../../.env -- npx prisma generate",
		"migrate": "dotenv -e ../../.env -- npx prisma migrate dev",
		"studio": "dotenv -e ../../.env -- npx prisma studio",
		"push": "dotenv -e ../../.env -- npx prisma db push",
		"deploy": "dotenv -e ../../.env -- npx prisma migrate deploy",
		"build": "tsup"
	},
	"tsup": {
		"entry": [
			"./src/index.ts"
		],
		"outDir": "../../dist/libs/db",
		"clean": true
	}
}

{
  "version": "1.0.0",
  "author": "Kenton Duprey",
  "private": true,
  "workspaces": [
    "./apps/*",
    "./packages/*"
  ],
  "scripts": {
    "vercel-build": "next build apps/site",
    "temp:preinstall": "npx only-allow pnpm",
    "postinstall": "cd ./packages/db && npx prisma generate && cd ../..",
    "build:site": "npm run -w 'apps/site' build",
    "start:site": "npm run -w 'apps/site' start",
    "dev:site": "npm run dev -w @bpvs/site",
    "cy:site": "npm run -w 'apps/site' cy:run",
    "build:admin": "npm run -w 'apps/admin' build",
    "start:admin": "npm run -w 'apps/admin' start",
    "dev:admin": "npm run -w 'apps/admin' dev",
    "build:cms": "npm run -w 'apps/cms' build",
    "start:cms": "npm run -w 'apps/cms' start",
    "dev:cms": "npm run -w 'apps/cms' dev",
    "publish:cms": "npm run -w 'apps/cms' publish",
    "pullData:cms": "npm run -w 'apps/cms' pullData",
    "start:db:local": "docker compose -f 'docker-compose.yml' up -d --build mongodb_container",
    "stop:db:local": "docker compose -f 'docker-compose.yml' down",
    "db:studio": "cd ./packages/db && dotenv -c -e ../../.env npx prisma studio",
    "db:local:studio": "vercel env pull && npm run start:db:local && npm run db:studio",
    "db:prod:studio": "vercel env pull --environment=production && npm run db:studio"
  },
  "dependencies": {
    "@bdsqqq/try": "^2.3.1",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.13.1",
    "graphql-request": "^6.1.0",
    "http-errors": "^2.0.0",
    "next": "^13.4.12",
    "raw-body": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.4",
    "stripe": "^12.16.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@bpvs/tsconfig": "*",
    "@bpvs/types": "*",
    "@types/http-errors": "^2.0.1",
    "@types/node": "^20.4.5",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "cypress": "^12.17.1",
    "eslint-config-bpvs": "*",
    "prettier": "^3.0.0",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tslib": "^2.6.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.3",
    "vitest": "^0.33.0",
    "vitest-mock-extended": "^1.1.3"
  },
  "packageManager": "npm@9.7.1"
}

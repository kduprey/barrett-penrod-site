{
  "name": "@bpvs/db",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "lint": "npx prisma format",
    "generate": "dotenv -c -e ../../.env run npx prisma generate",
    "migrate": "dotenv -c -e ../../.env  npx prisma migrate dev",
    "studio": "npx prisma studio",
    "push": "dotenv -c -e ../../.env npx prisma db push",
    "deploy": "dotenv -c -e ../../.env npx prisma migrate deploy",
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "./src/index.ts"
    ],
    "outDir": "../../dist/packages/db",
    "clean": true
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "dotenv": "^16.3.1",
    "json-schema-to-typescript": "^13.0.1"
  },
  "devDependencies": {
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.0.0"
  }
}

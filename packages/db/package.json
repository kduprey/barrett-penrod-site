{
	"name": "@bpvs/db",
	"version": "0.0.1",
	"private": true,
	"main": "./src/index.ts",
	"scripts": {
		"lint": "npx prisma format",
		"generate": "dotenv -c -e ../../.env npx prisma generate",
		"migrate": "dotenv -c -e ../../.env  npx prisma migrate dev",
		"studio": "npx prisma studio",
		"push": "dotenv -c -e ../../.env run npx prisma db push",
		"deploy": "dotenv -c -e ../../.env run npx prisma migrate deploy",
		"build": "tsup"
	},
	"dependencies": {
		"@prisma/client": "^5.3.0"
	},
	"devDependencies": {
		"@bpvs/types": "workspace:*",
		"dotenv": "^16.3.1",
		"eslint-config-bpvs": "workspace:*",
		"prisma": "^5.3.0",
		"tsconfig": "workspace:*"
	}
}

{
  "name": "@bpvs/db",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "lint": "npx prisma format",
    "generate": "dotenv -c -e ../../.env run npx prisma generate",
    "migrate": "dotenv -c -e ../../.env  npx prisma migrate dev",
    "studio": "npx prisma studio",
    "push": "dotenv -c -e ../../.env run npx prisma db push",
    "deploy": "dotenv -c -e ../../.env run npx prisma migrate deploy",
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "./src/index.ts"
    ],
    "outDir": "../../dist/packages/db",
    "clean": true
  }
}
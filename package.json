{
  "version": "1.0.0",
  "author": "Kenton Duprey",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "vercel-build": "next build apps/site",
    "temp:preinstall": "npx only-allow pnpm",
    "postinstall": "cd ./packages/db && npx prisma generate && cd ../..",
    "build:site": "npm run -w 'apps/site' build",
    "start:site": "npm run -w 'apps/site' start",
    "dev:site": "npm run -w 'apps/site' dev",
    "build:admin": "npm run -w 'apps/admin' build",
    "start:admin": "npm run -w 'apps/admin' start",
    "dev:admin": "npm run -w 'apps/admin' dev",
    "cy:site": "npm run -w 'apps/site' cy:run"
  },
  "dependencies": {
    "@bdsqqq/try": "^2.3.1",
    "axios": "^1.4.0",
    "framer-motion": "^10.12.16",
    "graphql-request": "^6.1.0",
    "http-errors": "^2.0.0",
    "next": "^13.4.7",
    "raw-body": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.1",
    "stripe": "^12.9.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@bpvs/tsconfig": "*",
    "@bpvs/types": "*",
    "@types/http-errors": "^2.0.1",
    "@types/node": "^20.3.1",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/coverage-c8": "^0.32.2",
    "@vitest/coverage-v8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "cypress": "^12.14.0",
    "eslint-config-bpvs": "*",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tslib": "^2.5.3",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2",
    "vitest-mock-extended": "^1.1.3"
  },
  "packageManager": "npm@9.7.1"
}
